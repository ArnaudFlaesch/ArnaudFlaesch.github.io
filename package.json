{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.1.2",
  "description": "Portfolio professionnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "build": "export NODE_ENV=production && gatsby build",
    "test:ci": "run-p -r start cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:run:ci": "wait-on http://localhost:8000 && npm run cy:run -- --config-file cypress-test.config.ts",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint src/",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "dotenv": "16.0.1",
    "gatsby": "4.15.2",
    "gatsby-plugin-image": "2.15.1",
    "gatsby-plugin-manifest": "4.15.1",
    "gatsby-plugin-offline": "5.15.1",
    "gatsby-plugin-react-helmet": "5.15.0",
    "gatsby-plugin-sass": "5.15.1",
    "gatsby-plugin-sharp": "4.15.1",
    "gatsby-remark-copy-linked-files": "5.15.0",
    "gatsby-remark-images": "6.15.1",
    "gatsby-remark-prismjs": "6.15.0",
    "gatsby-remark-responsive-iframe": "5.15.0",
    "gatsby-remark-smartypants": "5.15.0",
    "gatsby-source-filesystem": "4.15.0",
    "gatsby-source-graphql": "4.15.0",
    "gatsby-transformer-remark": "5.15.1",
    "gatsby-transformer-sharp": "4.15.1",
    "prismjs": "1.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.10.0",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "10.4.7",
    "cypress": "10.1.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "gatsby-plugin-postcss": "5.15.0",
    "gh-pages": "4.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.0",
    "sass": "1.52.3",
    "stylelint": "14.9.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "4.0.0",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.3",
    "wait-on": "6.0.1"
  }
}
