{
  "name": "portfolio",
  "private": true,
  "description": "A simple portfolio for developers",
  "version": "0.1.2",
  "engines": {
    "node": ">=14.0.0 <=15.12.0",
    "npm": "7.9.0"
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop -o",
    "ci": "run-p -r start cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "wait-on http://localhost:8000 && cypress run --headless --browser chrome",
    "cy:run:ci": "npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "reset": "rm -rf .cache/ public/",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "axios": "0.21.1",
    "dotenv": "6.2.0",
    "formik": "2.2.6",
    "gatsby": "2.32.12",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-canonical-urls": "2.10.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-nprogress": "2.10.0",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-plugin-sitemap": "2.12.0",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-source-graphql": "2.14.0",
    "gatsby-transformer-sharp": "2.12.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-anchor-link-smooth-scroll": "1.0.12",
    "react-dom": "16.14.0",
    "react-google-recaptcha": "1.1.0",
    "react-helmet": "5.2.1",
    "styled-components": "4.4.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.4",
    "@types/mocha": "8.2.2",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-eslint": "9.0.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-gatsby": "1.2.0",
    "cypress": "^7.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-wesbos": "0.0.23",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "1.7.0",
    "prettier": "2.2.1",
    "typescript": "4.2.4",
    "wait-on": "5.3.0"
  }
}
