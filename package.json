{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.2.0",
  "description": "Site web personnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "build": "export NODE_ENV=production && gatsby build",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:verify": "npx cypress verify",
    "lint:md": "markdownlint **/*.md",
    "lint:md:fix": "npm run lint:md -- --fix",
    "eslint": "eslint src/",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@formspree/react": "2.5.1",
    "@mui/icons-material": "5.15.10",
    "@mui/material": "5.15.10",
    "date-fns": "3.3.1",
    "dotenv": "16.4.4",
    "gatsby": "5.13.3",
    "gatsby-plugin-feed": "5.13.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-offline": "6.13.1",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sass": "6.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-remark-copy-linked-files": "6.13.1",
    "gatsby-remark-images": "7.13.1",
    "gatsby-remark-prismjs": "7.13.1",
    "gatsby-remark-responsive-iframe": "6.13.1",
    "gatsby-remark-smartypants": "6.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-source-graphql": "5.13.1",
    "gatsby-transformer-remark": "6.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "cypress": "13.6.4",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-markdownlint": "0.5.0",
    "eslint-plugin-prettier": "5.1.3",
    "gh-pages": "6.1.1",
    "markdownlint-cli": "0.39.0",
    "prettier": "3.2.5",
    "sass": "1.71.0",
    "stylelint": "16.2.1",
    "stylelint-config-standard-scss": "13.0.0",
    "typescript": "5.3.3"
  }
}
