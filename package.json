{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.1.2",
  "description": "Portfolio professionnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "start:ci": "export NODE_ENV=development && npm run start",
    "build": "export NODE_ENV=production && gatsby build",
    "test:ci": "run-p -r start:ci cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:run:ci": "wait-on http://localhost:8000 && npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "reset": "rm -rf .cache/ public/",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "dotenv": "8.6.0",
    "gatsby": "^3.13.1",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-source-graphql": "3.14.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.11",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "^10.3.4",
    "babel-plugin-istanbul": "6.0.0",
    "babel-preset-gatsby": "1.14.0",
    "cypress": "8.4.1",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gh-pages": "3.2.3",
    "prettier": "2.4.1",
    "sass": "^1.41.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3",
    "wait-on": "6.0.0"
  }
}