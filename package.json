{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.2.0",
  "description": "Site web personnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "build": "export NODE_ENV=production && gatsby build",
    "lint:md": "markdownlint **/*.md",
    "lint:md:fix": "npm run lint:md -- --fix",
    "eslint": "eslint src/",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:verify": "npx cypress verify",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "overrides": {
    "webpack-dev-middleware": "7.4.2"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fontsource/roboto": "5.1.0",
    "@formspree/react": "2.5.1",
    "@mui/icons-material": "6.1.1",
    "@mui/material": "6.1.1",
    "date-fns": "4.1.0",
    "dotenv": "16.4.5",
    "gatsby": "5.13.7",
    "gatsby-plugin-feed": "5.13.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-offline": "6.13.3",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sass": "6.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-remark-copy-linked-files": "6.13.2",
    "gatsby-remark-images": "7.13.2",
    "gatsby-remark-prismjs": "7.13.2",
    "gatsby-remark-responsive-iframe": "6.13.2",
    "gatsby-remark-smartypants": "6.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-source-graphql": "5.13.1",
    "gatsby-transformer-remark": "6.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "i18next": "23.15.1",
    "i18next-browser-languagedetector": "8.0.0",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.0.2",
    "react-photo-view": "1.2.6"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@eslint/markdown": "github:eslint/markdown",
    "@types/node": "20.16.8",
    "cypress": "13.14.2",
    "cypress-multi-reporters": "1.6.4",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.36.1",
    "gh-pages": "6.1.1",
    "globals": "15.9.0",
    "markdownlint-cli": "0.42.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.3.3",
    "sass": "1.79.3",
    "stylelint": "16.9.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0"
  }
}
