{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.1.2",
  "description": "Portfolio professionnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "build": "export NODE_ENV=production && gatsby build",
    "start": "gatsby develop -o",
    "start:ci": "export NODE_ENV=development && npm run start",
    "ci": "run-p -r start:ci cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:run:ci": "wait-on http://localhost:8000 && npm run cy:run -- --config-file cypress-test.json",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "reset": "rm -rf .cache/ public/",
    "test": "npm run cy:run"
  },
  "dependencies": {
    "axios": "0.21.1",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "gatsby": "3.7.0",
    "gatsby-image": "3.7.1",
    "gatsby-plugin-canonical-urls": "3.7.0",
    "gatsby-plugin-google-analytics": "3.7.0",
    "gatsby-plugin-manifest": "3.7.0",
    "gatsby-plugin-nprogress": "3.7.0",
    "gatsby-plugin-offline": "4.7.0",
    "gatsby-plugin-react-helmet": "4.7.0",
    "gatsby-plugin-sharp": "3.7.0",
    "gatsby-plugin-sitemap": "4.3.1",
    "gatsby-plugin-styled-components": "4.7.0",
    "gatsby-source-filesystem": "3.7.1",
    "gatsby-source-graphql": "3.7.1",
    "gatsby-transformer-sharp": "3.7.0",
    "gh-pages": "3.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-anchor-link-smooth-scroll": "1.0.12",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-helmet": "6.1.0",
    "styled-components": "5.3.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.6",
    "@types/mocha": "8.2.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-gatsby": "1.7.0",
    "cypress": "7.5.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-wesbos": "1.0.1",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.3.1",
    "typescript": "4.3.2",
    "wait-on": "5.3.0"
  }
}