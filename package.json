{
  "name": "arnaudflaesch.github.io",
  "author": "Arnaud Flaesch",
  "version": "0.1.2",
  "description": "Portfolio professionnel",
  "homepage": "https://arnaudflaesch.github.io",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "start": "gatsby develop -o",
    "build": "export NODE_ENV=production && gatsby build",
    "test:ci": "run-p -r start cy:run:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:run:ci": "wait-on http://localhost:8000 && npm run cy:run -- --config-file cypress-test.config.ts",
    "cy:verify": "npx cypress verify",
    "eslint": "eslint src/",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "npm run cy:run",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "gatsby": "5.12.5",
    "gatsby-plugin-image": "3.12.0",
    "gatsby-plugin-manifest": "5.12.0",
    "gatsby-plugin-offline": "6.12.0",
    "gatsby-plugin-react-helmet": "6.12.0",
    "gatsby-plugin-sass": "6.12.0",
    "gatsby-plugin-sharp": "5.12.0",
    "gatsby-remark-copy-linked-files": "6.12.0",
    "gatsby-remark-images": "7.12.0",
    "gatsby-remark-prismjs": "7.12.0",
    "gatsby-remark-responsive-iframe": "6.12.0",
    "gatsby-remark-smartypants": "6.12.0",
    "gatsby-source-filesystem": "5.12.0",
    "gatsby-source-graphql": "5.12.0",
    "gatsby-transformer-remark": "6.12.0",
    "gatsby-transformer-sharp": "5.12.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "6.1.7",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "autoprefixer": "10.4.16",
    "cypress": "13.3.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "5.0.0",
    "gatsby-plugin-postcss": "6.12.0",
    "gh-pages": "6.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "sass": "1.69.0",
    "stylelint": "15.10.3",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "wait-on": "7.0.1"
  }
}